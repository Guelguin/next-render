{
    "version": 2,
    "regions": ["dub1"],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, max-stale, s-maxage=5, stale-while-revalidate=30"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/rss",
            "destination": "/api/rss"
        },
        {
            "source": "/rss/fd",
            "destination": "https://fd.nl?rss"
        },
        {
            "source": "/rss/fd/:section",
            "destination": "https://fd.nl/$1/?rss"
        }
    ]
}
